   rdfs:comment "Aggiunge la relazione ha_lavorato_in per l'attore  in modo che la regola HaLavoratoCon restituisca i risultati voluti"^^xsd:string ;
   rdfs:label "AttoreHaLavoratoIn"^^xsd:string ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
