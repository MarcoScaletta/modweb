###  http://www.semanticweb.org/friday/ontologies/cinema#attore_in
:attore_in rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :ha_attore ;
           rdfs:domain :Attore ,
                       <http://xmlns.com/foaf/0.1/Person> ;
           rdfs:range <http://dbpedia.org/ontology/Film> ,
                      <http://dbpedia.org/ontology/Work> ,
                      :Film ,
                      [ owl:intersectionOf ( <http://dbpedia.org/ontology/Film>
                                             [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :ha_regista ;
                                                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                      owl:onClass :Regista
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :ha_sceneggiatore ;
                                                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
